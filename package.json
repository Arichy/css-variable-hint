{
  "name": "css-variable-hint",
  "displayName": "css-variable-hint",
  "description": "",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "icon": "resources/logo.png",
  "publisher": "arichy",
  "repository": {
    "url": "https://github.com/Arichy/css-variable-hint"
  },
  "main": "./out/client/extension.js",
  "contributes": {
    "configuration": {
      "title": "CSS Variable Hint",
      "properties": {
        "cssVariableHint.lookupFiles": {
          "type": "array",
          "description": "Glob for css files which contain css variables",
          "default": [
            "{,!(node_modules)/**/}*.css",
            "{,!(node_modules)/**/}*.sass",
            "{,!(node_modules)/**/}*.scss",
            "{,!(node_modules)/**/}*.less"
          ]
        }
      }
    },
    "commands": [
      {
        "command": "css-variable-hint.restartServer",
        "title": "CSS Variable Hint: Restart Server"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "fast-glob": "^3.2.11",
    "postcss": "^8.4.16",
    "postcss-scss": "^4.0.4",
    "vscode-css-languageservice": "^6.0.1",
    "vscode-languageclient": "^8.0.2",
    "vscode-languageserver": "^8.0.2",
    "vscode-languageserver-textdocument": "^1.0.5",
    "vscode-uri": "^3.0.3"
  }
}
